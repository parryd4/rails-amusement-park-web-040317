<%= form_for @user do |f| %>
<label>Name</label><br>
<%= f.text_field :name %><br>
<label>Password</label><br>
<%= f.text_field :password %><br>
<label>Height</label><br>
<%= f.text_field :height %><br>
<label>Happy</label><br>
<%= f.text_field :happiness %><br>
<label>Nausea</label><br>
<%= f.text_field :nausea %><br>
<label>Dollars</label><br>
<%= f.text_field :tickets %><br>
<label>Admin</label>
<%= f.check_box :admin %><br>
<%= f.submit %>
<% end %>
